*{
    padding: 0;
    margin: 0;
    box-sizing: border-box;
    font-family: system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;
}



h1{
    font-family: system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;
    margin-left: 260px;
    color: white;
}

header{
	position : fixed;
    height: 80px;
    background-size:cover;
    background-position: center;
    background-color: black;
}


ul{
    list-style: none;
    float:right;
    display: flex;
    margin-top: 30px;
    margin-right: 480px;
	margin-left: 480px;
}

ul li a{
    text-decoration: none;
    color: white;
    position: relative;
    padding: 15px 20px;
    border: 5px solid;
    border-color: white;
    border-radius: 50px;
    
}

body{
	overflow-y: scroll;
    background-color: rgb(49, 49, 49);
}

p{
    font-family: system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;
    margin-left: 260px;
    color: white;
}